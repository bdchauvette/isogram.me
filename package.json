{
  "name": "isogram.me",
  "version": "0.1.0",
  "description": "Web frontend for isogram Google Analytics generator",
  "private": true,
  "main": "build/app.js",
  "scripts": {
    "clean": "rimraf build/**/*",
    "test": "babel-tape-runner test/**/*.spec.js",
