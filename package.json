    "analytics",
    "tracking"
  ],
  "author": "Ben Chauvette <bdchauvette@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bdchauvette/isogram.me/issues"
  },
  "homepage": "https://github.com/bdchauvette/isogram.me#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-native-modules": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "baobab": "^2.3.3",
    "baobab-react": "^2.0.0",
    "classnames": "^2.2.5",
    "cross-env": "^1.0.7",
