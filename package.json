    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.9.1",
    "react": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "react-copy-to-clipboard": "^4.1.0",
    "react-dom": "^15.0.2",
    "rimraf": "^2.5.2",
